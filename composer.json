{
    "name": "hry/ayfer_integration_continue",
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "require": {
        "php": "^8.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.5",
        "phpstan/phpstan": "^2.1",
        "friendsofphp/php-cs-fixer": "^3.89"
    },
    "scripts": {
        "test": "vendor/bin/phpunit",                 
        "stan-check": "vendor/bin/phpstan analyse --no-progress --memory-limit=1G",      
        "cs-check": "vendor/bin/php-cs-fixer fix --dry-run --diff", 
        "cs-fix": "vendor/bin/php-cs-fixer fix" 
    }
}
